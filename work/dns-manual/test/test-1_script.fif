#!/usr/bin/env fift -s
"TonUtil.fif" include
"Asm.fif" include

0 constant workchain
103040 constant destination

"domain1" constant str-val

str-val $>s str-val $len 8 * u@ constant uint-val
uint-val .dump cr

dictnew constant dns-dict
<b workchain destination addr, b> <s uint-val dns-dict str-val $len 8 * udict!

drop
constant dns-dict

